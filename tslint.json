{
  "extends": ["tslint-config-airbnb", "tslint-react"],
  "rules": {
    "import-name": [true, { "styledComponents": "styled", "react": "React" }],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "ter-arrow-parens": [true, "as-needed"]
  }
}
